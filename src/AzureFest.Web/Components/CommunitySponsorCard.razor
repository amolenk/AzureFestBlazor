<div class="sponsor-logo">
    <a href="@Url" target="_blank">
        <img src="@ImageUrl" class="img-fluid" alt="@Name">
    </a>
</div>

@code
{
    [Parameter]
    public string? Name { get; set; }

    [Parameter]
    public string? Url { get; set; }

    [Parameter]
    public string? ImageUrl { get; set; }
}